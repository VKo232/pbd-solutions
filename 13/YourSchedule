public class StillUsingVariables { 
    public static void main(String[] args) {
        String[] classes = {"COURSE", "Biology", "Calculus", "Computer Science", "Physics"};
        String[] teachers = {"TEACHER", "Ms.Pulla", "Ms.De Gregorio", "Mr.Gallo", "Mr.Leduc"};
        for(int i = 0; i < classes.length; i++) {
            System.out.printf("|%-20s|%-20s|\n",classes[i], teachers[i]);
        }
    }
}
