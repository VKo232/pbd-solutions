import java.util.Random;

public class FortuneCookie {
    public static void main(String[] args) {
        Random rand = new Random();
        
        int num = 1 + rand.nextInt(6);
        
        switch (num) {
            case 1:  System.out.println("Fortune cookie says: Very good luck today");
                     break;
            case 2:  System.out.println("Fortune cookie says: Good luck today");
                     break;
            case 3:  System.out.println("Fortune cookie says: Slightly good luck today");
                     break;
            case 4:  System.out.println("Fortune cookie says: Bad good luck today");
                     break;
            case 5:  System.out.println("Fortune cookie says: Catastrophic good luck today");
                     break;
            case 6:  System.out.println("Fortune cookie says: No good luck today");
                     break;
            default: break;
        }

        for(int i = 0; i < 6; i++) {
            System.out.print((rand.nextInt(54) + 1) + " ");
        }
    }    
}
