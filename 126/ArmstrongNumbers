public class ArmstrongNumbers {
    public static void main(String[] args) {
        for(int i = 1; i < 10; i++) {
            for(int j = 0; j < 10; j++) {
                for(int k = 0; k < 10; k++) {
                    if(Math.pow(k, 2) + Math.pow(j , 2) + Math.pow(i , 2) == i * 100 + j * 10 + k) {
                        System.out.println(i + "" + j + "" + k);
                    }
                }
            }
        }
    }
}
